{
    "name": "spatie/laravel-api-logger",
    "description": ":description",
    "keywords": [
        "spatie",
        "laravel-api-logger"
    ],
    "homepage": "https://github.com/spatie/laravel-api-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Bobby Bouwmann",
            "email": "bobbybouwmann@gmail.com",
            "homepage": "https://bobbybouwmann.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/config": "~5.8.0",
        "illuminate/log": "^5.8",
        "illuminate/support": "~5.8.0"
    },
    "require-dev": {
        "ext-json": "*",
        "larapack/dd": "^1.0",
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ApiLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ApiLogger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ApiLogger\\ApiLoggerServiceProvider"
            ]
        }
    }
}
